{
    "name": "restgalleries/restgalleries",
    "type": "library",
    "description": "Is an API Client interface for interact with restful image services like Flickr through CRUD methods.",
    "license": "MIT",
    "authors": [
        {
            "name": "Matias Esteban",
            "email": "estebanmatias92@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "guzzle/guzzle": "3.8.0",
        "commerceguys/guzzle-oauth2-plugin": "dev-master",
        "doctrine/cache": "1.4.*",
        "illuminate/support": "4.1.*",
        "estebanmatias92/ohmy-auth": "0.0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*",
        "hamcrest/hamcrest-php": "~1.1"
    },
    "autoload": {
        "psr-0": {
            "RestGalleries": "src/",
            "RestGalleries\\Tests": "tests/"
        },
        "classmap": [
            "src/RestGalleries/APIs"
        ],
        "files": [
            "src/RestGalleries/Support/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
